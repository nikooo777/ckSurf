public Action Admin_DeleteCheckpoints(int client, int args)
{
	for (int i = 0; i < MAXPLAYERS + 1; i++)
		for (int x = 0; x < MAXZONEGROUPS; x++)
			for (int k = 0; k < CPLIMIT; k++)
				g_fCheckpointTimesRecord[x][i][k] = 0.0;

	db_deleteCheckpoints();
}